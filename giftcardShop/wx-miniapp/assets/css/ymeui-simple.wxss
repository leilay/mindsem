/* === split === */

.split {
  display: flex;
  position: relative;
  align-items: center;
  font-size: 9pt;
  color: rgba(0, 0, 0, 0.54);
}

.split::before, .split::after {
  content: " ";
  flex: 1;
  display: block;
  height: 1px;
  background-color: rgba(0, 0, 0, 0.12);
}

.split::before {
  margin-right: 1em;
}

.split::after {
  margin-left: 1em;
}

/* === amount-block === */

.amount-block {
  font-weight: lighter;
  font-size: 12px;
}

.amount-content {
  padding: 24px 0;
}

.amount-title::before, .amount-title::after {
  content: " ";
  display: inline-block;
  width: 2em;
  height: 1px;
  background-color: rgba(255, 255, 255, 0.54);
  top: -.3em;
  position: relative;
}

.amount-title::before {
  margin-right: 0.5em;
}

.amount-title::after {
  margin-left: 0.5em;
}

.amount-footer {
  padding: 8px 0;
}

.amount-footer .item {
  position: relative;
}

.amount-footer .item:not(:last-child)::after {
  content: " ";
  display: block;
  position: absolute;
  right: 0;
  top: 25%;
  width: 1px;
  height: 50%;
  background-color: rgba(255, 255, 255, 0.54);
}

.bg-white .item:not(:last-child)::after {
  background-color: rgba(0, 0, 0, 0.12);
}

/*------------------------------
* biz style
------------------------------*/

.line1, .line2, .line3 {
  position: relative;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  display: -webkit-inline-box;
}

/* 1行字... */

.line1 {
  -webkit-line-clamp: 1;
  max-height: 1.5em;
}

/* 两行字... */

.line2 {
  -webkit-line-clamp: 2;
  max-height: 3em;
}

/* 三行字... */

.line3 {
  -webkit-line-clamp: 3;
  max-height: 4.5em;
}

/* === app-list === */

.app-list .app-item {
  text-align: center;
  padding: 8px 0;
  position: relative;
}

.app-list .item {
  padding: 24px 16px;
  position: relative;
  border-bottom: 1px solid rgba(0, 0, 0, 0.05);
  line-height: 1.2;
}

.separate-item>view::after {
  content: " ";
  display: block;
  position: absolute;
  top: 19.1%;
  right: 0;
  height: 61.8%;
  width: 1px;
  background: rgba(0, 0, 0, 0.05);
}

.app-list .app-icon {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
}

/* === goods-list === */

.goods-list {
  zoom: 1;
  overflow: hidden;
}

.goods-list>.item {
  width: calc(50% - 4px);
  float: left;
  background-color: #fff;
  margin: 2px;
}

.goods-list>.item .item-media {
  height: calc(50vw - 4px);
}

.goods-list>.item .item-media image {
  width: 100%;
  height: 100%;
}

.goods-list>.item .item-inner {
  padding: 4px;
  min-height: 80px;
  position: relative;
}

.goods-list>.item .item-title {
  height: 3em;
  /* margin-bottom: 0.5em; */
}

.goods-list>.item .badges {
  height: 24px;
  overflow: hidden;
}

.goods-list>.item .item-price {
  line-height: 24px;
  font-size: 32rpx;
}

/* goods-list-block */

.goods-list-block .item {
  display: flex;
  width: 100%;
  margin: 1px 0;
}

.goods-list-block>.item .item-media {
  width: 32vw;
  height: 32vw;
}

.goods-list-block>.item .item-inner {
  flex: 1;
  padding-left: 48rpx;
}

.add2cart {
  position: absolute;
  right: 24rpx;
  bottom: 16rpx;
  border: 1px solid #eee;
  padding: 16rpx;
  border-radius: 8rpx;
  transition: all 0.2s linear 0s;
}

.add2cart:hover {
  background-color: rgba(0, 0, 0, 0.12);
}

/* === cart === */

.cart {
  font-size: 24rpx;
}

.cart-group + .cart-group {
  margin-top: 32rpx;
}

.cart .group-header {
  position: sticky;
  top: 0;
  padding: 12px 8px 12px 16px;
  background-color: #f5f5f9;
  z-index: 15;
}

.cart .group-header checkbox {
  margin-right: 8px;
}

.cart .item-content.media .item-inner {
  align-self: stretch;
}

.cart .item-title {
  font-size: 9pt;
}

.cart .media {
  margin: 8px 0;
}

.cart .media-group:not(:last-child) {
  border-bottom: 1px solid rgba(0, 0, 0, 0.12);
}

.cart .media-group-caption {
  padding: 8px;
  font-size: 9pt;
}

.cart .media-group-caption .tag {
  margin-right: 4px;
}

.cart .media .item-inner {
  padding-bottom: 24px;
}

.cart .media .item-price {
  position: absolute;
  bottom: 0;
  width: 100%;
}

.cart .goods-list-block .item-selector {
  display: block;
}

/* === coupon  === */

/* coupon-wx */

.coupon-wx .card-header::after {
  display: none;
}

.coupon-wx .coupon-header {
  width: 100%;
  margin-top: -96rpx;
}

.coupon-wx .coupon-split {
  position: relative;
  height: 0;
  border-top: 1px dashed rgba(0, 0, 0, 0.12);
  margin-top: -1px;
  z-index: 99;
}

.coupon-wx .coupon-split::before, .coupon-wx .coupon-split::after {
  content: " ";
  display: block;
  position: absolute;
  top: -8px;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  z-index: 15;
}

.coupon-wx .coupon-split::before {
  left: -8px;
}

.coupon-wx .coupon-split::after {
  right: -8px;
  left: auto;
}

.coupon-wx .coupon-header image {
  width: 160rpx;
  height: 160rpx;
  max-height: 160rpx;
}

/* === card msg === */

.card.msg .card-content .hd {
  line-height: 1.2;
}

.card.msg .card-content .hd .desc {
  margin-top: 16px;
}

.card.msg .card-content .bd {
  padding: 16px 0;
}

/* === vip-card === */

.vip-card {
  border-radius: 8px;
  overflow: hidden;
  color: #fff;
  text-shadow: 1px 1px 0 rgba(0, 0, 0, 0.54);
}

.vip-card .card-content {
  height: 180px;
  background-size: cover;
  padding-top: 8px;
}

.vip-card image {
  border: 2px solid #fff;
}

.vip-card .item-title {
  font-size: 16pt;
}

/* === splash === */

.splash-container {
  height: 100vh;
  width: 100vw;
}

.splash-container swiper {
  /* only for wx  */
  height: 100vh;
}

.splash-img {
  height: 100vh;
}

/* === news list === */

.news-list .card-header-sm .item-media {
  margin-right: 8px;
}

.news-list {
  padding: 16px 0;
}

.news-list .card+.card {
  margin-top: 8px;
}

.news-list .card-header-sm {
  border: none;
  line-height: 24px;
}

.news-list .card-content-inner {
  padding: 8px 16px;
}

.news-list .news-item-v>.card-content-inner {
  display: flex;
}

.news-list .news-item-h>.card-content-inner .item-media {
  padding: 8px 0;
}

.news-list .news-item-h>.card-content-inner .item-media image {
  height: 43vw;
}

.news-list .news-item-v>.card-content-inner .item-title {
  flex: 1;
  padding-right: 16px;
}

.news-list .news-item-v>.card-content-inner .item-media {
  width: 34vw;
}

.news-list .news-item-v>.card-content-inner .item-media image {
  height: 25vw;
}

.news-list .news-item-h>.card-content-inner .item-media+.caption {
  margin-bottom: 8px;
}

.news-list .item-footer {
  font-size: 9pt;
  color: rgba(0, 0, 0, 0.54);
}

.news-list .news-item-v .item-footer {
  position: absolute;
  bottom: 8px;
  left: 16px;
  right: 16px;
}

.news-list .news-item-v .item-footer .float-right {
  background-color: rgba(0, 0, 0, 0.54);
  color: #fff;
  padding: 0 6px;
}

/*------------------------------
* colors
------------------------------*/

page {
  background-color: #f5f5f9;
}

.content-block-title {
  color: rgba(0, 0, 0, 0.54);
  background-color: #f5f5f9;
}

/* list-block */

.list-block {
  background-color: #fff;
}

.list-block .item-content:not(:last-child) .item-inner::after {
  background-color: rgba(0, 0, 0, 0.05);
}

.list-group-title {
  background-color: rgba(0, 0, 0, 0.05);
}

/* card */

.card-header:after, .card-footer:before {
  background-color: rgba(0, 0, 0, 0.05);
}

/* badge */

.badge {
  color: #fff;
  background: #8e8e93;
}

/* tag colors */

.tag-danger {
  color: #f30;
  border-color: #f30;
}

/* timeline colors */

.timeline-item {
  color: rgba(0, 0, 0, 0.54);
}

.timeline .timeline-item:first-child .timeline-item-content::after {
  background-color: #06ccab;
}

.timeline .timeline-item:first-child {
  color: rgba(0, 0, 0, 0.87);
}

.timeline .timeline-item-content .time {
  color: rgba(0, 0, 0, 0.54);
}

.timeline-progress .timeline-item.current::before,
.timeline-progress .timeline-item.current~.timeline-item::after,
.timeline-progress .timeline-item.current~.timeline-item::before {
  background-color: #06ccab;
}

.timeline-progress .timeline-item.current .timeline-item-content::after,
.timeline-progress .timeline-item.current+.timeline-item .timeline-item-content::after {
  background-color: #06ccab;
}

.timeline-progress .timeline-item.current .item-title,
.timeline-progress .timeline-item.current+.timeline-item .item-title {
  color: rgba(0, 0, 0, 0.87);
}

/* coupon-wx */

.coupon-bg {
  background-color: #06ccab;
}

.coupon-wx .coupon-split::before {
  background-color: #f5f5f9;
  border-right: 1px solid #ccc;
}

.coupon-wx .coupon-split::after {
  background-color: #f5f5f9;
  border-left: 1px solid #ccc;
}

/* style for my-app  */

.banner {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
}

.footer {
  background-color: transparent;
}

.closed {
  -webkit-filter: grayscale(100%);
}

.tabbar-main {
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  color: #999;
  z-index: 54;
}

.tabbar-main .item {
  padding-top: 16rpx;
  height: 96rpx;
}

.tabbar-main .item.active {
  color: #06ccab;
}

.tabbar-main .new-button {
  width: 120rpx;
  height: 64rpx;
  padding: 8rpx 0;
  background-color: #ddd;
  border-radius: 32rpx;
  display: inline-block;
  position: relative;
  top: -32rpx;
  border: 16rpx solid #fff;
  box-sizing: content-box;
  box-shadow: 0 -8rpx 8rpx 0 rgba(0, 0, 0, 0.05);
}

.tabbar-main .new-button .icon {
  width: 64rpx;
  height: 64rpx;
}

/* number-editor */

.number-editor {
  height: 48rpx;
  overflow: hidden;
  vertical-align: top;
  border: 1px solid #eee;
}

.number-editor button {
  padding: 0 16rpx;
  border-radius: 0;
  float: left;
}

.number-editor button::after {
  display: none;
}

.number-editor input {
  float: left;
  width: 80rpx;
  font-size: 24rpx;
  height: 40rpx;
  line-height: 40rpx;
  min-height: auto;
  overflow: hidden;
  text-align: center;
}

/* uploader */

.uploader {
  width: 100%;
  text-align: center;
}

.uploader image {
  background-color: #eee;
}

.uploader .icon-camera {
  margin-top: 28rpx;
}

.uploader .upload {
  height: 20vw;
  background-color: #999;
}
